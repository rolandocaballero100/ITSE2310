#! usr/bin/env swift

// Range Operators and Loops
// Department Store Program
// Send discount vouchers to customers between ages 18 and 30

// firstnumber...lastnumber

// closed range Operators
let myRange = 10...20

for Range in 10...20{
	print(Range)
}

print("")

// half-open range Operator
let myRange2 = 10..<20 

for Range2 in 10..<20 {
	print(Range2) q
}